{
  "name": "respirators-101",
  "private": true,
  "version": "1.0.0",
  "description": "Just breathe",
  "main": "index.js",
  "scripts": {
    "test": "tape -r reify ./**/*-test.js",
    "prep-build": "run-p build-twine-data-to-decisions-html build-wordpress-data-to-svelte create-symlinks glob",
    "build": "npm-run-all prep-build --parallel build:*",
    "build:js": "rollup -c",
    "watch:js": "rollup -c -w",
    "build:css": "postcss-alt \"client/routes/**/*.css\" \"./public/style.css\"",
    "watch:css": "postcss-alt \"client/routes/**/*.css\" \"./public/style.css\" --watch=\"client/**/*.css\"",
    "glob": "run-p glob:*",
    "glob:routes": "glob-module-file --pattern=\"client/routes/**/*.js\" --format=es --pathPrefix=../../ --outputPath=client/data/globbed-routes.js",
    "glob:content": "glob-module-file --pattern=\"client/data/content/*.html\" --format=es --pathPrefix=../../ --exportWithPath --outputPath=client/data/globbed-content.js",
    "create-symlinks": "symlink client/lib client/node_modules/lib && symlink client/data client/node_modules/data",
    "build-twine-data-to-decisions-html": "node twine-parser/output-decisions-html-object.js",
    "build-wordpress-data-to-svelte": "node wordpress-parser/xml-to-svelte.js",
    "dev:server": "live-server public",
    "dev": "npm-run-all prep-build --parallel watch:* dev:server",
    "fetch-blind-polyfill": "curl 'http://127.0.0.1:3000/v2/polyfill.min.js?features=default-3.6,NodeList.prototype.@@iterator,NodeList.prototype.forEach,RegExp.prototype.flags&flags=always,gated' -H 'User-Agent: Mozilla/5.0 Firefox/900.0' > public/blind-polyfill.js",
    "cordova-init": "cd cordova && npx cordova platform add android && npx cordova platform add ios",
    "cordova-update-config": "cd cordova && npx cordova prepare",
    "cordova-copy-www": "rm -rf cordova/www; mkdir cordova/www && cp -r public/* cordova/www && node cordova-copy.js",
    "cordova-build-ios": "cd cordova && npx cordova build ios",
    "cordova-build-android": "cd cordova && npx cordova build android",
    "cordova-test-android": "adb -s ZY223MJ3P7 install -r cordova/platforms/android/build/outputs/apk/debug/android-debug.apk",
    "cordova-android": "npm-run-all cordova-build-android cordova-test-android"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/respirators-101.git"
  },
  "author": "Cool dudes",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/TehShrike/respirators-101/issues"
  },
  "homepage": "https://github.com/TehShrike/respirators-101#readme",
  "dependencies": {
    "abstract-state-router": "^6.0.4",
    "better-emitter": "^1.0.2",
    "better-match": "^1.0.2",
    "better-replace": "^1.0.2",
    "date-and-time": "^0.6.2",
    "fuse.js": "^3.2.0",
    "pify": "^3.0.0",
    "regex-fun": "^2.0.2",
    "svelte": "^2.13.4",
    "svelte-state-renderer": "^2.0.0",
    "validator": "^9.4.0"
  },
  "devDependencies": {
    "glob-module-file": "^2.2.0",
    "globby": "^7.0.0",
    "loud-rejection": "^1.6.0",
    "he": "^1.1.1",
    "cheerio": "^1.0.0-rc.2",
    "@rgrove/parse-xml": "^1.1.1",
    "download": "^6.2.5",
    "symlink-cli": "^1.0.0",
    "reify": "^0.12.3",
    "postcss-alt-cli": "^1.0.0",
    "precss": "^2.0.0",
    "parse5": "^3.0.3",
    "make-dir": "^1.1.0",
    "make-dir-cli": "^1.0.0",
    "markdown-it": "^8.4.1",
    "autoprefixer": "^7.1.6",
    "babel-core": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "google-fonts-offline": "^0.1.2",
    "npm-run-all": "^4.1.2",
    "tape": "4.8.0",
    "tape-catch": "^1.0.6",
    "remarkable": "^1.7.1",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-svelte": "4.1.0",
    "balanced-match": "^1.0.0",
    "cordova": "^7.1.0",
    "gaze": "^1.1.2",
    "html-tags": "^2.0.0",
    "jsoncomp": "^1.6.1",
    "live-server": "^1.2.0",
    "parse-gitignore": "^0.4.0",
    "postcss-color-function": "^4.0.1",
    "posthtml": "^0.11.3",
    "posthtml-custom-elements": "^1.1.2",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-posthtml": "^1.0.1",
    "rollup-plugin-progress": "^0.4.0",
    "runjs": "^4.3.2",
    "simple-ssh": "^1.0.0",
    "svelte-preprocess-postcss": "^1.0.0"
  }
}
