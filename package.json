{
  "name": "respirators-101",
  "version": "1.0.0",
  "description": "Just breathe",
  "main": "index.js",
  "scripts": {
    "test": "tape -r reify ./**/*-test.js",
    "prep-build": "run-p build-twine-data-to-decisions-html build-wordpress-data-to-svelte create-symlinks glob",
    "build": "npm-run-all prep-build --parallel build:*",
    "build:js": "rollup -c",
    "watch:js": "rollup -c -w",
    "build:css": "postcss-alt \"client/routes/**/*.css\" \"./public/style.css\"",
    "watch:css": "postcss-alt \"client/routes/**/*.css\" \"./public/style.css\" --watch=\"client/**/*.css\"",
    "glob": "run-p glob:*",
    "glob:routes": "glob-module-file --pattern=\"client/routes/**/*.js\" --format=es --pathPrefix=../../ --outputPath=client/data/globbed-routes.js",
    "glob:content": "glob-module-file --pattern=\"client/data/content/*.html\" --format=es --pathPrefix=../../ --exportWithPath --outputPath=client/data/globbed-content.js",
    "create-symlinks": "symlink client/lib client/node_modules/lib && symlink client/data client/node_modules/data",
    "build-twine-data-to-decisions-html": "node twine-parser/output-decisions-html-object.js",
    "build-wordpress-data-to-svelte": "node wordpress-parser/xml-to-svelte.js",
    "dev:server": "serve public",
    "dev": "npm-run-all prep-build --parallel watch:* dev:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/respirators-101.git"
  },
  "author": "Cool dudes",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/TehShrike/respirators-101/issues"
  },
  "homepage": "https://github.com/TehShrike/respirators-101#readme",
  "dependencies": {
    "@rgrove/parse-xml": "^1.1.1",
    "abstract-state-router": "^6.0.4",
    "autoprefixer": "^7.1.6",
    "babel-core": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "better-match": "^1.0.2",
    "better-replace": "^1.0.1",
    "download": "^6.2.5",
    "glob-module-file": "^2.2.0",
    "globby": "^7.0.0",
    "he": "^1.1.1",
    "loud-rejection": "^1.6.0",
    "make-dir": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "pify": "^3.0.0",
    "postcss-alt-cli": "^1.0.0",
    "precss": "^2.0.0",
    "regex-fun": "^2.0.2",
    "reify": "^0.12.3",
    "remarkable": "^1.7.1",
    "rollup": "0.51.5",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-svelte": "3.1.0",
    "svelte": "1.41.3",
    "svelte-state-renderer": "^1.2.5",
    "symlink-cli": "^1.0.0",
    "tape": "4.8.0",
    "tape-catch": "^1.0.6"
  },
  "devDependencies": {
    "gaze": "^1.1.2",
    "serve": "^6.4.1",
    "watch": "^1.0.2"
  }
}
